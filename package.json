{
  "name": "boiler-plate",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@azure/storage-blob": "^12.11.0",
    "@types/amqplib": "^0.8.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.10",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/newrelic": "^9.4.0",
    "@types/node": "^16.11.56",
    "@types/nodemailer": "^6.4.4",
    "@types/object-hash": "^2.2.1",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.0",
    "@types/validator": "^13.7.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.9",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@google-cloud/translate": "^8.0.1",
    "@vitalets/google-translate-api": "^9.2.0",
    "aws-sdk": "^2.1167.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.1",
    "colors.ts": "^1.0.20",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "google-translate-api": "^2.3.0",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "newrelic": "^9.7.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.7.7",
    "object-hash": "^3.0.0",
    "path": "^0.12.7",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "stripe": "^10.10.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "tslog": "^3.3.4",
    "tsyringe": "^4.7.0",
    "twilio": "^3.77.3",
    "typeorm": "0.3.9",
    "uuid": "^9.0.0",
    "validator": "^13.11.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "host_permissions": [
    "https://translate.google.com/"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "server": "node ./src/index.js",
    "migration:generate": "node  -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -o -d ./src/data-source.js -p",
    "migration:run": "node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.js",
    "migration:revert": "node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/data-source.js",
    "migration:dev:generate": "ts-node  -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/data-source.ts -p",
    "migration:dev:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts",
    "migration:dev:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/data-source.ts",
    "start": "ts-node -r tsconfig-paths/register -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon -e ts --exec ts-node -r tsconfig-paths/register ./src/index.ts"
  }
}
